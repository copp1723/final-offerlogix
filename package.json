  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "vite build",
    "build:render": "vite build --config vite.config.render.ts",
    "start": "NODE_ENV=production tsx server/index.ts",
    "check": "echo 'TypeScript checking disabled for now'",
    "typecheck": "echo 'TypeScript checking disabled for now'",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "test": "jest",
    "test:v2:mailgun": "jest --config server/v2/jest.v2.cjs",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:v2-bridge": "playwright test --config=tests/e2e/v2-bridge-playwright.config.ts",
    "test:v2-bridge:headed": "playwright test --config=tests/e2e/v2-bridge-playwright.config.ts --headed",
    "test:v2-bridge:debug": "playwright test --config=tests/e2e/v2-bridge-playwright.config.ts --debug",
    "setup:supermemory": "node scripts/setup-supermemory.js",
    "test:supermemory": "node scripts/test-supermemory.js",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:push": "drizzle-kit push",
    "db:sql": "for f in drizzle/0002_*.sql; do [ -f \"$f\" ] && echo Running $f && psql $DATABASE_URL -v ON_ERROR_STOP=1 -f $f || true; done",
    "db:setup": "./scripts/setup-database.sh",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:all": "node scripts/run-all-migrations.js",
    "db:migrate:status": "node scripts/run-all-migrations.js --status",
    "db:migrate:backup": "node scripts/run-all-migrations.js --backup",
    "db:doctor": "tsx server/scripts/db-doctor.ts",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed:test": "NODE_ENV=test tsx scripts/seed-database.ts",
    "db:migrate-prod": "tsx scripts/migrate-production-data.ts",
    "db:reset-dev": "tsx scripts/reset-dev-data.ts",
    "db:reset-quick": "tsx scripts/reset-dev-data.ts --quick",
    "auth:setup": "tsx server/scripts/create-admin-user.ts",
    "auth:test": "node test-authentication-system.js",
    "auth:reset-admin": "tsx server/scripts/create-admin-user.ts --reset",
    "security:test": "node test-security-implementation.js",
    "test:setup": "./scripts/test-setup.sh",
    "deploy:hook": "./scripts/deploy-hook.sh"
  },
